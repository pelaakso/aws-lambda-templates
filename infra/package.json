{
  "name": "aws-lambda-templates",
  "version": "0.1.0",
  "bin": {
    "infra": "bin/infra.js"
  },
  "engines": {
    "node": ">20.0.0 <23.0.0",
    "npm": ">=9.0.0"
  },
  "scripts": {
    "all": "npx eslint . && npx prettier --check . && npm run test",
    "build": "tsc --noEmit",
    "clean": "npx rimraf '+(bin|lib|functions|test|utils)/**/+(*.d.ts|*.js)'",
    "watch": "tsc -w",
    "snap": "jest",
    "test": "npm run build && jest",
    "cdk": "npx cdk"
  },
  "devDependencies": {
    "@eslint/eslintrc": "^3.3.0",
    "@eslint/js": "^9.21.0",
    "@types/aws-lambda": "^8.10.147",
    "@types/jest": "^29.5.14",
    "@types/node": "22.13.9",
    "@typescript-eslint/eslint-plugin": "^8.26.0",
    "@typescript-eslint/parser": "^8.26.0",
    "aws-cdk": "^2.1003.0",
    "esbuild": "^0.25.1",
    "eslint": "^9.21.0",
    "eslint-config-prettier": "^10.1.1",
    "eslint-plugin-prettier": "^5.2.3",
    "globals": "^16.0.0",
    "jest": "^29.7.0",
    "prettier": "^3.5.3",
    "ts-jest": "^29.2.6",
    "ts-node": "^10.9.2",
    "typescript": "^5.8.2"
  },
  "dependencies": {
    "aws-cdk-lib": "^2.182.0",
    "constructs": "^10.4.2",
    "source-map-support": "^0.5.21"
  }
}
