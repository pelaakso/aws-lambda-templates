{
  "name": "aws-lambda-templates",
  "version": "0.1.0",
  "bin": {
    "infra": "bin/infra.js"
  },
  "engines": {
    "node": ">20.0.0 <23.0.0",
    "npm": ">=9.0.0"
  },
  "scripts": {
    "all": "npx eslint . && npx prettier --check . && npm run test",
    "build": "tsc --noEmit",
    "clean": "npx rimraf '+(bin|lib|functions|test|utils)/**/+(*.d.ts|*.js)'",
    "watch": "tsc -w",
    "snap": "jest",
    "test": "npm run build && jest",
    "cdk": "npx cdk"
  },
  "devDependencies": {
    "@eslint/eslintrc": "^3.3.3",
    "@eslint/js": "^9.39.2",
    "@types/aws-lambda": "^8.10.159",
    "@types/jest": "^30.0.0",
    "@types/node": "^22.19.2",
    "@typescript-eslint/eslint-plugin": "^8.49.0",
    "@typescript-eslint/parser": "^8.49.0",
    "aws-cdk": "^2.1100.1",
    "esbuild": "^0.25.10",
    "eslint": "^9.39.2",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-prettier": "^5.5.4",
    "globals": "^16.5.0",
    "jest": "^30.2.0",
    "prettier": "^3.7.4",
    "ts-jest": "^29.4.6",
    "ts-node": "^10.9.2",
    "typescript": "^5.9.3"
  },
  "dependencies": {
    "aws-cdk-lib": "^2.232.2",
    "constructs": "^10.4.4",
    "source-map-support": "^0.5.21"
  }
}
