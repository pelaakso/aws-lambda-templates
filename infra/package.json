{
  "name": "aws-lambda-templates",
  "version": "0.1.0",
  "bin": {
    "infra": "bin/infra.js"
  },
  "engines": {
    "node": ">22.0.0 <25.0.0",
    "pnpm": ">=10.16.0",
    "npm": "\n\nUse pnpm!!\n\n"
  },
  "scripts": {
    "all": "pnpm run eslint && pnpm run prettier && pnpm run test",
    "build": "tsc --noEmit",
    "clean": "pnpx rimraf '+(bin|lib|functions|test|utils)/**/+(*.d.ts|*.js)'",
    "eslint": "eslint .",
    "npm-check-updates": "npm-check-updates",
    "prettier": "prettier --check .",
    "watch": "tsc -w",
    "snap": "jest",
    "test": "pnpm run build && jest",
    "cdk": "cdk"
  },
  "devDependencies": {
    "@eslint/eslintrc": "^3.3.3",
    "@eslint/js": "^9.39.2",
    "@types/aws-lambda": "^8.10.160",
    "@types/jest": "^30.0.0",
    "@types/node": "^24.10.4",
    "@typescript-eslint/eslint-plugin": "^8.54.0",
    "@typescript-eslint/parser": "^8.54.0",
    "aws-cdk": "^2.1104.0",
    "esbuild": "^0.27.2",
    "eslint": "^9.39.2",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-prettier": "^5.5.5",
    "globals": "^17.2.0",
    "jest": "^30.2.0",
    "npm-check-updates": "^19.3.2",
    "prettier": "^3.8.1",
    "ts-jest": "^29.4.6",
    "ts-node": "^10.9.2",
    "typescript": "^5.9.3"
  },
  "dependencies": {
    "aws-cdk-lib": "^2.237.1",
    "constructs": "^10.4.5",
    "source-map-support": "^0.5.21"
  }
}
